{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ggsql",
      "camelcase": "ggsql",
      "title": "ggsql",
      "scope": "source.ggsql",
      "file-types": [
        "ggsql"
      ],
      "injection-regex": "^ggsql$",
      "class-name": "TreeSitterggsql"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "ggsql grammar for tree-sitter",
    "authors": [
      {
        "name": "ggsql Authors"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ggsql"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
